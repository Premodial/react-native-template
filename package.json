    "start": "node scripts/runScript.js start",
    "start-reset": "npm start -- --reset-cache",
    "start-devtools": "react-devtools",
    "load-env": "node scripts/runScript.js loadEnv",
    "create-env-file": "node scripts/runScript.js createEnvFile",
    "graphql-codegen": "node scripts/runScript.js graphqlCodegen",
    "android": "cross-env ENVFILE=.env.development IS_STORYBOOK=false node scripts/runScript.js runAndroid",
    "android:storybook": "cross-env ENVFILE=.env.storybook IS_STORYBOOK=true node scripts/runScript.js runAndroid",
    "ios": "cross-env ENVFILE=.env.development IS_STORYBOOK=false node scripts/runScript.js runIos",
    "ios:storybook": "cross-env ENVFILE=.env.storybook IS_STORYBOOK=true node scripts/runScript.js runIos",
    "build-android:app": "cross-env ENVFILE=.env.production node scripts/runScript.js buildAndroidApp",
    "bundle:analysis": "npx visualize-bundle",
    "test": "jest --detectOpenHandles",
    "test:noCache": "jest --detectOpenHandles --no-cache",
    "lint:code": "eslint \"*/**/*.{js,ts,tsx}\"",
    "lint:code:fix": "npm run lint:code -- --fix",
    "type-check": "tsc --noEmit",
    "precommit": "cross-env ENVFILE=.env.development node scripts/runScript.js precommit",
    "clean-install": "node ./scripts/cleanInstall",
    "npm:clean": "node scripts/runScript.js npmClean",
    "postinstall": "node scripts/runScript.js postinstall"
